<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x110"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/edittext_rect_white"
    android:layout_marginTop="@dimen/x6"
    >

    <ImageView
        android:id="@+id/iv_selector"
        android:background="@drawable/select_slod_blue"
        android:layout_width="@dimen/x20"
        android:layout_height="@dimen/x20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.14"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.464"
        app:layout_constraintWidth_percent="0.24"
        />

    <TextView
        android:id="@+id/tv_device_serics"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center|left"
        android:text="设备系列"
        android:textColor="@color/bg_black"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.19"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.184"
        app:layout_constraintWidth_percent="0.24" />

    <TextView
        android:id="@+id/tv_device_number"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="设备系列"
        android:textColor="@color/bg_black"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.19"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.184"
        app:layout_constraintWidth_percent="0.24" />

    <TextView
        android:id="@+id/tv_device_class"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center|left"
        android:text="设备系列"
        android:textColor="@color/bg_black"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.19"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.184"
        app:layout_constraintWidth_percent="0.24" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_device_rssi"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center|right"
        android:textColor="@color/bg_black"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.19"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.184"
        app:layout_constraintWidth_percent="0.1" />

    <TextView
        android:id="@+id/tv_room_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center|right"
        android:textSize="12dp"
        android:textColor="@color/bg_black"
        android:text="在仓"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintWidth_percent="0.1"
        app:layout_constraintHeight_percent="0.14"/>

    <TextView
        android:id="@+id/tv_device_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginRight="@dimen/x10"
        android:gravity="center|right"
        android:textSize="12dp"
        android:textColor="@color/bg_black"
        android:text="正常"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/tv_room_status"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.88"
        app:layout_constraintWidth_percent="0.1"
        app:layout_constraintHeight_percent="0.14"
        />

    <TextView
        android:id="@+id/tv_room_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center|left"
        android:text="深圳利物仓库"
        android:textColor="@color/bg_black"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.464"
        app:layout_constraintWidth_percent="0.24" />

    <TextView
        android:id="@+id/tv_series_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="设备系列"
        android:textColor="@color/bg_black"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintLeft_toLeftOf="@+id/tv_device_number"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.464"
        app:layout_constraintWidth_percent="0.5" />

    <TextView
        android:id="@+id/tv_series_scan_count"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:textColor="@color/bg_black"
        android:textSize="12dp"
        android:layout_marginRight="@dimen/x5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.464"
        app:layout_constraintWidth_percent="0.1" />

    <TextView
        android:id="@+id/tv_room_remark"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center|left"
        android:text="备注备注备注注备注备注备注备注备注备注"
        android:textColor="@color/bg_black"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.30"
        app:layout_constraintHorizontal_bias="0.806"
        app:layout_constraintLeft_toLeftOf="@+id/tv_device_serics"
        app:layout_constraintRight_toRightOf="@+id/tv_device_status"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.826" />
</androidx.constraintlayout.widget.ConstraintLayout>
